{
  "import": {
    "url": "https://www.openml.org/data/download/22102279/dataset",
    "input": "data/bronze/credit_risk_dataset.csv",
    "output": "data/bronze/bronze.parquet"
  },
  "preprocess": {
    "input": "data/bronze/bronze.parquet",
    "output": "data/silver/silver.parquet",
    "scaler": "standard"
  },
  "split": {
    "input": "data/silver/silver.parquet",
    "train_output": "data/gold/train.parquet",
    "test_output": "data/gold/test.parquet",
    "target_col": "loan_status",
    "test_size": 0.1,
    "random_state": 5910
  }
}
